{
	"name" : "omarpre/paubox",
	"description" : "A PHP package for the Paubox Transactional Email API (Fork of original paubox-php SDK)",
	"homepage" : "https://github.com/omarpre/paubox",
	"license" : "Apache-2.0",
	"require" : {
		"php": "^8.0.2 || ^8.1",
		"nategood/httpful" : "*",
		"vlucas/phpdotenv" : "^5.5.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^5.7.9"
	},
	"autoload" : {
		"psr-4" : {
			"Paubox\\" : "src/",
			"Paubox\\Mail\\" : "src/mail/",
			"Paubox\\Service\\" : "src/service/"
		}
	},
	"archive" : {
		"exclude" : [
			"/src/tests"
		]
	}
}